{
  "name": "@seapay/deeplink",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "bin": {
    "seapay-deeplink": "bin/seapay-deeplink.js"
  },
  "scripts": {
    "deeplink": "node bin/seapay-deeplink.js",
    "dev": "tsx src/cli.ts --help",
    "build": "tsc",
    "start": "node dist/cli.js --help",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "qr-code-styling": "^1.9.2",
    "tsx": "^4.21.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.0.0 || ^19.0.0",
    "typescript": "^5.9.3"
  }
}
